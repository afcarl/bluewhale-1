md-dialog(aria-label="Remove Image")
  md-content
    md-subheader(class="md-sticky-no-effect") Remove Image {{ imageShortId }}
    md-checkbox(ng-model="params.force" aria-label="Force") Force: {{ params.force }}
    md-checkbox(ng-model="params.noprune" aria-label="No Prune") No Prune: {{ params.noprune }}
    p {{ content }}

  div(class="md-actions" layout="row")
    md-button(ng-click="cancel()") Cancel
    md-button(ng-click="ok()" class="md-primary") Yes
