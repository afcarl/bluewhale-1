md-dialog.container-dialog(aria-label="Create an image")
  md-toolbar(layout="column" layout-align="center center")
    md-subheader.md-sticky-no-effect
      span.action Create 
      | an Image
  md-content
    md-list
      md-item
        md-text-float(label="From Image, name" ng-model="queryParams.fromImage")
      md-item
        md-text-float(label="From Src" ng-model="queryParams.fromSrc")
      md-item
        md-text-float(label="Repository" ng-model="queryParams.repo")
      md-item
        md-text-float(label="Tag" ng-model="queryParams.tag")
      md-item
        md-text-float(label="Registry" ng-model="queryParams.registry")
      md-item
        p {{ content }}

  div(class="md-actions" layout="row")
    md-button(ng-click="cancel()") Cancel
    md-button(ng-click="ok()" class="md-primary") Yes

