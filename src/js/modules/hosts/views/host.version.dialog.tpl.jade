md-dialog.host-dialog(aria-label="Docker Version")
  md-toolbar(layout="column" layout-align="center center")
    md-subheader.md-sticky-no-effect Docker Version
  md-content
    md-list
      md-item(ng-repeat="(key, value) in docker")
        md-divider(ng-if="!$first")
        md-item-content
          .md-tile-left(flex) {{ key }}
          .md-title-content(flex) {{ value }}
      md-item
      md-item

  div(class="md-actions" layout="row")
    md-button(ng-click="cancel()") Cancel
    md-button(ng-click="ok()" class="md-primary") Yes
